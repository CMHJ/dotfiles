{
  "python.pythonPath": "/bin/python",
  "workbench.colorTheme": "Nord",
  "files.trimTrailingWhitespace": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "security.workspace.trust.enabled": false,
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "git.confirmSync": false,
  "git.autofetch": true,
  "editor.rulers": [79],
  "redhat.telemetry.enabled": false,
  "python.linting.flake8Enabled": true,
  "workbench.editor.wrapTabs": true,
  "editor.lineNumbers": "relative",
  "python.linting.flake8Args": ["--max-line-length=180"],
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "editor.acceptSuggestionOnCommitCharacter": false,
  "typescript.suggest.completeFunctionCalls": true,
  "javascript.suggest.completeFunctionCalls": true,
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cmake.configureOnOpen": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terraform.languageServer": {
    "external": true,
    "pathToBinary": "",
    "args": [
      "serve"
    ],
    "ignoreSingleFileWarning": false
  },
  "python.formatting.provider": "black",
  "editor.fontSize": 18,
  "workbench.enableExperiments": false,
  "editor.fontFamily": "FantasqueSansMono Nerd Font Mono, 'monospace', monospace",
  "glassit-linux.opacity": 90,
  "glassit.alpha": 240,
  "cSpell.language": "en-AU",
  "cSpell.userWords": [
    "Deque"
  ],
  "spellright.notificationClass": "information",
  "spellright.language": [
    "en_AU"
  ]